function PromptPullFW(PULLFW, ForcePullFw) is x
 PULLFW        is x
 ForcePullFw   is x
 yes_btn       is x
 no_btn        is x

 PromptPullFw = "N"

 if ForcePullFw = "Y" or UIDid dp	'ER001 - add UIDid check, if passed - force the force, and may the force be with you... 
  PromptPullFw = "Y"
  return
 endif

 if PULLFW = "Y"
  $submitopt("off","Yes")
  $cancelopt("off","No")
  $form($funcname) 
   $bstyle("buttondown","button")
   $text("{H1 align='CENTER'}Would you like to use the existing ICD10 Diagnosis as a template for this update?{/H1}")
   $text(`"{CENTER}Last Diagnosis Date: {u}{b}{font color=red}" + $castx(c.dx10.dt) + "{/font}{/b}{/u}{/CENTER}"`)
   $br(2)
   $tag("<center>")
    $submit(yes_btn,"Yes")$ctag("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;")$submit(no_btn,"No")
   $tag("</center>") 
  $sendform($funcname) 
  if $endbutton = "SUBMIT" or yes_btn = "Y"
   PromptPullFW = "Y"
  endif 
 endif
end PromptPullFW

